{
  "name": "@skeepit/react-native-app-clip",
  "version": "1.0.12",
  "description": "Config plugin to add an App Clip to a React Native iOS app",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "ios",
    "plugin/build",
    "build",
    "README.md",
    "expo-module.config.json",
    "app.plugin.js"
  ],
  "scripts": {
    "ncu": "bun x npm-check-updates -p bun",
