  "keywords": [
    "react-native",
    "expo",
    "react-native-app-clip",
    "ReactNativeAppClip"
  ],
  "repository": "https://github.com/kezios/react-native-app-clip",
  "bugs": {
    "url": "https://github.com/bndkt/react-native-app-clip/issues"
  },
  "author": "@AlexandreFauchard @Kezios",
  "license": "MIT",
  "homepage": "https://github.com/bndkt/react-native-app-clip#readme",
  "dependencies": {
    "@expo/config-plugins": "latest",
    "@expo/plist": "^0.0.20"
  },
  "devDependencies": {
    "@types/react": "latest",
    "@types/react-native": "latest",
    "expo-module-scripts": "latest",
    "expo-modules-core": "latest",
    "prettier": "latest",
    "react": "latest",
    "react-native": "latest",
    "typescript": "latest"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  }
